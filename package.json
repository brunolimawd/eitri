{
  "name": "Eitri",
  "version": "1.0.0",
  "description": "Front-end framework made with â™¥ by humans",
  "main": "gulpfile.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint:jshint": "jshint --reporter node_modules/jshint-stylish/stylish.js scripts/app/**.js",
    "build:less": "lessc --source-map-url=style.map styles/less/style.less > styles/dist/style.css",
    "build:prefixer": "autoprefixer --annotation 'style.map' -b 'ie >= 8, ie_mob >= 10, ff >= 15, chrome >= 30, safari >= 5, opera >= 23, ios >= 6, android >= 2.3, bb >= 10' styles/dist/style.css",
    "build:css": "npm run build:less && npm run build:prefixer",
    "build": "npm run build:css",
    "watch:js": "watch 'npm run lint:jshint' scripts/app/ --wait=0",
    "watch:css": "watch 'npm run build:css' styles/less/ --wait=0",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:css'"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:brunolimawd/eitri.git"
  },
  "keywords": [
    "#eitri",
    "#h5bp"
  ],
  "author": "Humans",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^5.1.0",
    "browser-sync": "^1.3.6",
    "csso": "^1.3.11",
    "gulp": "^3.8.7",
    "gulp-autoprefixer": "0.0.10",
    "gulp-changed": "^1.0.0",
    "gulp-csso": "^0.2.9",
    "gulp-if": "^1.2.4",
    "gulp-imagemin": "^1.0.1",
    "gulp-jshint": "^1.8.4",
    "gulp-less": "^1.3.5",
    "gulp-load-plugins": "^0.6.0",
    "gulp-menu": "^0.2.2",
    "gulp-sourcemaps": "^1.5.2",
    "gulp-uglify": "^1.0.1",
    "jshint": "^2.6.3",
    "jshint-stylish": "^1.0.1",
    "less": "^2.4.0",
    "parallelshell": "^1.1.1",
    "psi": "^0.1.2",
    "watch": "^0.14.0"
  }
}
